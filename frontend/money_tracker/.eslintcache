ogin = (user) => {\n    this.setState({\n      currentUser: user\n    }) \n  }\n\n\n  render() {\n    return(\n      <div>\n        <BrowserRouter>\n          <Switch>\n           \n              \n\n            <Route\n              exact \n              path='/'\n              render={routeProps =>\n              <LoginPage \n              user ={this.state.currentUser}\n              {...routeProps}/>\n              } \n            />\n\n            <Route \n              exact \n              path='/login'\n              render={routeProps =>\n              <LoginForm\n              user ={this.state.currentUser}\n              handleLogin={this.handleLogin} \n              {...routeProps}/>\n              } \n            />\n\n            <Route \n              exact \n              path='/signup'\n              render={routeProps =>\n              <SignupForm \n              {...routeProps}/>\n              } \n            /> \n\n            <Route\n              exact\n              path='/MainPage'\n              render={routeProps =>\n              <MainPage\n              user={this.state.currentUser}\n              handleLogin={this.handleLogin}\n              {...routeProps}/>\n              }\n            />\n\n            <Route\n              exact\n              path='/UpdateInfo'\n              render={routeProps =>\n              <UpdateInfo\n              user={this.state.currentUser}\n              handleLogin={this.handleLogin}\n              {...routeProps}/>\n              }\n            />\n\n            <Route\n              exact\n              path='/Chart'\n              render={routeProps =>\n              <Chart\n              user={this.state.currentUser}\n              handleLogin={this.handleLogin}\n              {...routeProps}/>\n              }\n            />\n\n            \n             \n\n\n\n          </Switch>\n        </BrowserRouter>\n      </div>\n    )\n  }\n}\n \n\nexport default App;\n",["74","75"],"/Users/tomtom/Desk